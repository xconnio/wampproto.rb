module WampProto
  # message root
  module Message
    module Type
      HELLO: 1
      WELCOME: 2
      ABORT: 3
      CHALLENGE: 4
      AUTHENTICATE: 5
      GOODBYE: 6
    end

    HANDLER: ::Hash[Integer, singleton(Message::Base)]

    def self.resolve: (Array[untyped] wamp_message) -> Message::Base
  end
end

